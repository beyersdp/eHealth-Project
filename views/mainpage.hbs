					<p hidden>{{ text }} </p>
					<p hidden>{{ verbleibPatient }} </p>
					{{# if rettungsmittel}}
					{{# each rettungsmittel}}
						<p style="font-size:11px"><i class="fa fa-ambulance fa-fw w3-margin-right"></i>{{ funkruf }} <span class="fahrzeugArt">{{ art }}</span></p>
					{{/each}}
					{{/if}}
					
					{{# if posten}}
					{{# each posten}}
						<p style="font-size:11px"><i class="fa fa-flag fa-fw w3-margin-right"></i>{{ funkruf }}</p>
					{{/each}}
					{{/if}}
					
					{{# if rettungskraefte}}
					{{# each rettungskraefte}}
						<p style="font-size:11px"><i class="fa fa-user-md fa-fw w3-margin-right"></i>{{ funkruf }} <span class="medQuali">{{ quali }}</span></p>
					{{/each}}
					{{/if}}
				</div> <!-- id="{{ _id }}" -->
				<p></p>
				{{/if}}
				{{/each}}
			</div>
			{{/if}}

