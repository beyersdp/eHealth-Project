				{{# if aktiv}}
				<div class="einsatz dragElement w3-container w3-white w3-card w3-round w3-hover-shadow" id="{{ _id }}" draggable="true">
					<button class="w3-center w3-button w3-block w3-blue-grey w3-half" onclick="updateEinsatz('{{ _id }}')" style="height:30px; width:75%; font-size:12px;" title="Bearbeiten" id="{{ meldebild }}">{{ meldebild }}</button>
					<form method="POST" action="/einsatzClose" id="einsatzClose_form">
						<input type="hidden" name="einsatz_id" value="{{ _id }}"></input>
						<button type="submit" class="w3-center w3-button w3-block w3-blue-grey w3-quarter" style="height:30px; font-size:10px; border-left: 1px solid white;" title="Abschließen"><i class="fa fa-check-square-o"></i></button>
					</form>
					<p style="font-size:11px"><i class="fa fa-arrow-right fa-fw w3-margin-right"></i>{{ status }}</p>
					<p style="font-size:11px"><i class="fa fa-clock-o fa-fw w3-margin-right"></i><span class="timestamp">{{ timestamp }}</span> Uhr</p>
